<?xml version='1.0' encoding='utf-8'?>
<widget id="com.IanDilling.adventurebuddy" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="Accelerometer">
        <param name="android-package" value="org.apache.cordova.devicemotion.AccelListener" />
    </feature>
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.GeoBroker" />
    </feature>
    <name>AdventureBuddy</name>
    <description>
        An application used to find outdoor activites in the surrounding area.
    </description>
    <author email="ianbjorndilling@gmail.com" href="http://cordova.io">
        Ian Dilling
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="UIWebViewBounce" value="false" />
</widget>
